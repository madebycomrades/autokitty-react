{
  "name": "autokitty",
  "version": "0.0.1",
  "description": "This kitty sorts out your complicated group expenses",
  "scripts": {
    "postinstall": "npm run bundle && npm run uglify && npm run gzip",
    "start": "babel-node --stage 0 index.js",
    "start:dev": "NODE_ENV=development PORT=3000 npm start",
    "watch:server": "nodemon --watch index.js --watch server --watch db --ext js,json,hbs --exec 'npm run start:dev'",
    "watch:client": "npm run clean && WATCHIFY=true NODE_ENV=development babel-node --stage 0 browserify.js",
    "bundle": "npm run clean && NODE_ENV=production babel-node --stage 0 browserify.js",
    "uglify": "uglifyjs static/main.bundle.js -o static/main.bundle.js -m -c unused=false",
    "gzip": "babel-node --stage 0 gzip.js",
    "test": "babel-node --stage 0 test.js",
    "lint": "eslint .",
    "clean": "mkdir -p static && rm -rf static/main.bundle.js*"
  },
  "dependencies": {
    "babel": "5.8.20",
    "babel-core": "5.8.20",
    "babel-eslint": "4.0.5",
    "babel-runtime": "5.8.20",
    "babelify": "6.1.3",
    "browserify": "11.0.1",
    "co-body": "3.0.0",
    "envify": "3.4.0",
    "eslint": "1.0.0",
    "eslint-config-standard": "4.0.0",
    "eslint-config-standard-react": "1.0.3",
    "eslint-plugin-react": "3.2.0",
    "eslint-plugin-standard": "1.2.0",
    "handlebars": "3.0.3",
    "isomorphic-fetch": "2.1.1",
    "jasmine": "2.3.2",
    "jasmine-spec-reporter": "2.4.0",
    "jsdom": "5.6.1",
    "koa": "0.21.0",
    "koa-route": "2.4.2",
    "koa-static": "1.4.9",
    "memdown": "1.0.0",
    "nodemon": "1.4.0",
    "page": "1.6.3",
    "path-to-regexp": "1.2.0",
    "pouchdb": "4.0.0",
    "pouchdb-upsert": "1.1.1",
    "proxyquire": "1.6.0",
    "react": "0.13.3",
    "react-redux": "0.2.2",
    "redux": "1.0.0-rc",
    "reselect": "0.0.2",
    "rx-lite": "3.0.0",
    "shortid": "2.2.2",
    "to-slug-case": "0.1.2",
    "uglify-js": "2.4.24",
    "watchify": "3.3.1"
  },
  "engines": {
    "iojs": "2.3.0",
    "npm": "2.11.1"
  }
}
