{
  "name": "autokitty",
  "version": "0.0.1",
  "description": "This kitty sorts out your complicated group expenses",
  "config": {
    "localdb": ".db/autokitty"
  },
  "scripts": {
    "postinstall": "npm run bundle",
    "start": "babel-node --stage 0 index.js",
    "dev:start": "NODE_ENV=development PORT=3000 npm start",
    "watch:server": "nodemon --watch index.js --watch views --watch db --ext js,json,hbs --exec 'npm run dev:start'",
    "watch:client": "WATCHIFY=true babel-node --stage 0 browserify.js",
    "bundle": "babel-node --stage 0 browserify.js",
    "test": "babel-node --stage 0 test.js",
    "lint": "eslint test.js index.js browserify.js public views db jasmine-helpers"
  },
  "dependencies": {
    "babel": "^5.5.4",
    "babel-core": "^5.5.4",
    "babel-eslint": "^3.1.9",
    "babel-runtime": "^5.5.4",
    "babelify": "^6.1.2",
    "browserify": "^10.2.4",
    "co-body": "^2.0.0",
    "eslint": "^0.22.1",
    "eslint-plugin-react": "^2.3.0",
    "flummox": "^4.0.0-alpha",
    "handlebars": "^3.0.2",
    "immutable": "^3.7.2",
    "isomorphic-fetch": "^2.0.2",
    "jasmine": "^2.3.1",
    "jasmine-spec-reporter": "^2.2.1",
    "jsdom": "^5.4.1",
    "koa": "^0.21.0",
    "koa-route": "^2.4.1",
    "koa-static": "^1.4.9",
    "memdown": "^1.0.0",
    "nodemon": "^1.3.7",
    "pouchdb": "^3.4.0",
    "pouchdb-upsert": "^1.1.1",
    "proxyquire": "^1.4.0",
    "react": "^0.13.2",
    "react-router": "^0.13.3",
    "shortid": "^2.2.2",
    "to-slug-case": "^0.1.2",
    "watchify": "^3.2.2"
  },
  "engines": {
    "iojs": "2.0.0",
    "npm": "2.9.0"
  },
  "jspm": {
    "directories": {
      "baseURL": "public"
    },
    "dependencies": {},
    "devDependencies": {
      "babel": "npm:babel-core@^5.1.13",
      "babel-runtime": "npm:babel-runtime@^5.1.13",
      "core-js": "npm:core-js@^0.9.4"
    }
  }
}
