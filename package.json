{
  "name": "autokitty",
  "version": "0.0.1",
  "description": "This kitty sorts out your complicated group expenses",
  "scripts": {
    "postinstall": "npm run bundle && npm run uglify && npm run gzip",
    "start": "babel-node --stage 0 index.js",
    "start:dev": "NODE_ENV=development PORT=3000 npm start",
    "watch:server": "nodemon --watch index.js --watch views --watch db --ext js,json,hbs --exec 'npm run start:dev'",
    "watch:client": "WATCHIFY=true NODE_ENV=development babel-node --stage 0 browserify.js",
    "bundle": "mkdir -p static && babel-node --stage 0 browserify.js",
    "uglify": "uglifyjs static/main.bundle.js -o static/main.bundle.js -m -c unused=false",
    "gzip": "babel-node --stage 0 gzip.js",
    "test": "babel-node --stage 0 test.js",
    "lint": "eslint ."
  },
  "dependencies": {
    "babel": "5.6.23",
    "babel-core": "5.7.4",
    "babel-eslint": "3.1.23",
    "babel-runtime": "5.7.0",
    "babelify": "6.1.3",
    "browserify": "11.0.0",
    "co-body": "2.0.0",
    "envify": "3.4.0",
    "eslint": "0.24.1",
    "eslint-plugin-react": "2.7.1",
    "handlebars": "3.0.3",
    "isomorphic-fetch": "2.1.1",
    "jasmine": "2.3.1",
    "jasmine-spec-reporter": "2.4.0",
    "jsdom": "5.6.1",
    "koa": "0.21.0",
    "koa-route": "2.4.2",
    "koa-static": "1.4.9",
    "memdown": "1.0.0",
    "nodemon": "1.3.7",
    "page": "1.6.3",
    "path-to-regexp": "1.2.0",
    "pouchdb": "3.6.0",
    "pouchdb-upsert": "1.1.1",
    "proxyquire": "1.6.0",
    "react": "0.13.3",
    "react-redux": "0.2.2",
    "redux": "^1.0.0-rc",
    "rx-lite": "2.5.2",
    "shortid": "2.2.2",
    "to-slug-case": "0.1.2",
    "uglify-js": "2.4.23",
    "watchify": "3.3.0"
  },
  "engines": {
    "iojs": "2.3.0",
    "npm": "2.11.1"
  }
}
